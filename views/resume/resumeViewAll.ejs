<html>
    <head>
        <title>Resume</title>
    </head>
    <body>
        <h1>Resume</h1>
        <table border="1">
            <tr><th>Resume ID</th><th>User Account ID</th><th>Resume Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><%= result[i].resume_id%></td>
                <td><%= result[i].user_account_id %></td>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].resume_name %></a></td>

            </tr>
            <% } %>
        </table>
    </body>

</html>